// svgcharts.jade
extends layout
block css
  each css in assets.svgcharts.css
    link(rel="stylesheet", href=css, type="text/css")

block js
  each js in assets.svgcharts.js
    script(type="text/javascript", src=js)

block body
  .container-fluid
    .row
      .col-sm-12
        #svgcharts-view

